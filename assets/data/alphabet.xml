<!DOCTYPE codename-engine-alphabet>
<letters fps="24">
	<bold spritesheet="menus/alphabet-bold">
		<!-- Bold Letters !-->
		<letter char="A"        anim="character-a" />
		<letter char="B"        anim="character-b" />
		<letter char="C"        anim="character-c" />
		<letter char="D"        anim="character-d" />
		<letter char="E"        anim="character-e" />
		<letter char="F"        anim="character-f" />
		<letter char="G"        anim="character-g" />
		<letter char="H"        anim="character-h" />
		<letter char="I"        anim="character-i" />
		<letter char="J"        anim="character-j" />
		<letter char="K"        anim="character-k" />
		<letter char="L"        anim="character-l" />
		<letter char="M"        anim="character-m" />
		<letter char="N"        anim="character-n" />
		<letter char="O"        anim="character-o" />
		<letter char="P"        anim="character-p" />
		<letter char="Q"        anim="character-q" />
		<letter char="R"        anim="character-r" />
		<letter char="S"        anim="character-s" />
		<letter char="T"        anim="character-t" />
		<letter char="U"        anim="character-u" />
		<letter char="V"        anim="character-v" />
		<letter char="W"        anim="character-w" />
		<letter char="X"        anim="character-x" />
		<letter char="Y"        anim="character-y" />
		<letter char="Z"        anim="character-z" />

		<letter char="&amp;"    anim="character-ampersand" />
		<letter char="&lt;"     anim="character-anglebracket-left" />
		<letter char="&gt;"     anim="character-anglebracket-right" />
		<letter char="*"        anim="character-asterisk" />
		<letter char="@"        anim="character-at" />
		<letter char="\"        anim="character-backslash" />
		<letter char="`"        anim="character-backtick" />
		<letter char="^"        anim="character-caret" />
		<letter char=":"        anim="character-colon" />
		<letter char=","        anim="character-comma" />
		<letter char="{"        anim="character-curlybracket-left" />
		<letter char="}"        anim="character-curlybracket-right" />
		<letter char="$"        anim="character-dollar" />
		<letter char='"'        anim="character-doublequote" />
		<letter char="="        anim="character-equal" />
		<letter char="!"        anim="character-exclamationmark" />
		<letter char="-"        anim="character-hyphen" />
		<letter char="%"        anim="character-percent" />
		<letter char="."        anim="character-period" />
		<letter char="+"        anim="character-plus" />
		<letter char="#"        anim="character-pound" />
		<letter char="?"        anim="character-questionmark" />
		<letter char="("        anim="character-roundbracket-left" />
		<letter char=")"        anim="character-roundbracket-right" />
		<letter char=";"        anim="character-semicolon" />
		<letter char="'"        anim="character-singlequote" />
		<letter char="/"        anim="character-slash" />
		<letter char="["        anim="character-squarebracket-left" />
		<letter char="]"        anim="character-squarebracket-right" />
		<letter char="~"        anim="character-tilde" />
		<letter char="_"        anim="character-underscore" />
		<letter char="|"        anim="character-verticalbar" />

		<letter char="0"        anim="character-zero" />
		<letter char="1"        anim="character-one" />
		<letter char="2"        anim="character-two" />
		<letter char="3"        anim="character-three" />
		<letter char="4"        anim="character-four" />
		<letter char="5"        anim="character-five" />
		<letter char="6"        anim="character-six" />
		<letter char="7"        anim="character-seven" />
		<letter char="8"        anim="character-eight" />
		<letter char="9"        anim="character-nine" />
	</bold>
	<normal spritesheet="menus/alphabet">
		<!-- Symbols !-->
		<letter char="$"        anim="dollarsign" />
		<letter char="#"        anim="hashtag" />
		<letter char="%"        anim="%" />
		<letter char="&amp;"    anim="amp" />
		<letter char="("        anim="(" />
		<letter char=")"        anim=")" />
		<letter char="*"        anim="*"        center="middle" />
		<letter char="+"        anim="+"        center="middle" />
		<letter char="-"        anim="-"        center="middle" />
		<letter char="0"        anim="0" />
		<letter char="1"        anim="1" />
		<letter char="2"        anim="2" />
		<letter char="3"        anim="3" />
		<letter char="4"        anim="4" />
		<letter char="5"        anim="5" />
		<letter char="6"        anim="6" />
		<letter char="7"        anim="7" />
		<letter char="8"        anim="8" />
		<letter char="9"        anim="9" />
		<letter char=":"        anim=":" />
		<letter char=";"        anim=";" />
		<letter char="<"        anim="<" />
		<letter char=">"        anim=">" />
		<letter char="="        anim="=" />
		<letter char="@"        anim="@" />
		<letter char="|"        anim="|" />
		<letter char="~"        anim="~" />
		<letter char="["        anim="[" />
		<letter char="]"        anim="]" />
		<letter char="→"        anim="right arrow"  center="middle" />
		<letter char="←"        anim="left arrow"   center="middle" />
		<letter char="↓"        anim="down arrow"   center="middle" />
		<letter char="↑"        anim="up arrow"     center="middle" />
		<letter char="."        anim="period"       center="middle" />
		<letter char="?"        anim="question mark" />
		<letter char="&#34;"    anim="start parentheses" />

		<!-- Capital Letters !-->
		<letter char="A"        anim="A capital" />
		<letter char="B"        anim="B capital" />
		<letter char="C"        anim="C capital" />
		<letter char="D"        anim="D capital" />
		<letter char="E"        anim="E capital" />
		<letter char="F"        anim="F capital" />
		<letter char="G"        anim="G capital" />
		<letter char="H"        anim="H capital" />
		<letter char="I"        anim="I capital" />
		<letter char="J"        anim="J capital" />
		<letter char="K"        anim="K capital" />
		<letter char="L"        anim="L capital" />
		<letter char="M"        anim="M capital" />
		<letter char="N"        anim="N capital" />
		<letter char="O"        anim="O capital" />
		<letter char="P"        anim="P capital" />
		<letter char="Q"        anim="Q capital" />
		<letter char="R"        anim="R capital" />
		<letter char="S"        anim="S capital" />
		<letter char="T"        anim="T capital" />
		<letter char="U"        anim="U capital" />
		<letter char="V"        anim="V capital" />
		<letter char="W"        anim="W capital" />
		<letter char="X"        anim="X capital" />
		<letter char="Y"        anim="Y capital" />
		<letter char="Z"        anim="Z capital" />

		<!-- Lowercase Letters !-->
		<letter char="a"        anim="a lowercase" />
		<letter char="b"        anim="b lowercase" />
		<letter char="c"        anim="c lowercase" />
		<letter char="d"        anim="d lowercase" />
		<letter char="e"        anim="e lowercase" />
		<letter char="f"        anim="f lowercase" />
		<letter char="g"        anim="g lowercase" />
		<letter char="h"        anim="h lowercase" />
		<letter char="i"        anim="i lowercase" />
		<letter char="j"        anim="j lowercase" />
		<letter char="k"        anim="k lowercase" />
		<letter char="l"        anim="l lowercase" />
		<letter char="m"        anim="m lowercase" />
		<letter char="n"        anim="n lowercase" />
		<letter char="o"        anim="o lowercase" />
		<letter char="p"        anim="p lowercase" />
		<letter char="q"        anim="q lowercase" />
		<letter char="r"        anim="r lowercase" />
		<letter char="s"        anim="s lowercase" />
		<letter char="t"        anim="t lowercase" />
		<letter char="u"        anim="u lowercase" />
		<letter char="v"        anim="v lowercase" />
		<letter char="w"        anim="w lowercase" />
		<letter char="x"        anim="x lowercase" />
		<letter char="y"        anim="y lowercase" />
		<letter char="z"        anim="z lowercase" />
	</normal>
</letters>