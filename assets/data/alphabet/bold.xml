	<anim char='"' y="35">character-doublequote0</anim>
	<anim char="=" y="20">character-equal</anim>
	<anim char="!">character-exclamationmark0</anim>
	<anim char="-" y="25">character-hyphen</anim>
	<anim char="%">character-percent</anim>
	<anim char=".">character-period</anim>
	<anim char="+" y="10">character-plus</anim>
